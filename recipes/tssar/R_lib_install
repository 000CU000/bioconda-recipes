required.packages<-""
new.packages <- required.packages[!(required.packages %in% installed.packages()[,"Package"])]
if(length(new.packages)) install.packages(new.packages, repos='http://cran.us.r-project.org')
required.packages<-"skellam"
if(length(required.packages[!(required.packages %in% installed.packages()[,"Package"])])) install.packages("skellam_0.0-8-7.tar.gz", repos=NULL)